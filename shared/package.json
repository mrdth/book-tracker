{
  "name": "book-tracker-shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.js"
    },
    "./queries/*": {
      "types": "./dist/queries/*.d.ts",
      "import": "./dist/queries/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
